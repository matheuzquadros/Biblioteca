@model IEnumerable<Biblioteca.Models.Cliente>

<h1>Clientes</h1>
<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col" class="text-center">@Html.DisplayNameFor(model => model.Nome)</th>
            <th scope="col" class="text-center">@Html.DisplayNameFor(model => model.Email)</th>
            <th scope="col" class="text-center">@Html.DisplayNameFor(model => model.Cpf)</th>
            <th scope="col" class="text-center">@Html.DisplayNameFor(model => model.DataNascimento)</th>
            <th scope="col" class="text-center">@Html.DisplayNameFor(model => model.Senha)</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => item.Nome)</td>
                <td>@Html.DisplayFor(modelItem => item.Email)</td>
                <td>@Html.DisplayFor(modelItem => item.Cpf)</td>
                <td>@Html.DisplayFor(modelItem => item.DataNascimento)</td>
                <td>@Html.DisplayFor(modelItem => item.Senha)</td>
            </tr>
        }
    </tbody>
</table>